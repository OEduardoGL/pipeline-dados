{
  "app_name": "Pipe2 Demo",
  "paths": {
    "raw": "data/raw",
    "bronze": "data/bronze",
    "silver": "data/silver",
    "gold": "data/gold"
  },
  "csv_options": {
    "header": true,
    "delimiter": ",",
    "mode": "PERMISSIVE"
  },
  "spark": {
    "logLevel": "WARN",
    "config": {
      "spark.sql.shuffle.partitions": "4",
      "spark.master": "local[2]"
    }
  },
  "write": {
    "mode": "overwrite",
    "partitionBy_orders": ["order_date"],
    "partitionBy_gold": ["order_date"]
  }
}
